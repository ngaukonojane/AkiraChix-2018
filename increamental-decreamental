#include <iostream>

using namespace std;

int main()
{
    int x = 5;
    for (int i = 0;i < 5;i++){
    cout << x--;
    cout << "\n";
    }
cout<<"\n";
x=5;
for (int i= 0; i <5; i ++){
    cout << --x;
    cout << "\n";
}

return 0;
}









